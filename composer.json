{
    "name": "php-kitchen/yii2-di",
    "description": "Advanced Dependency Injection container built on top of default Yii2 container.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Yii2",
        "Yii",
        "DI",
        "Dependency Injection",
        "Container",
        "Service Provider",
        "Decorator",
        "Factory"
    ],
    "authors": [
        {
            "name": "Dmitry Kolodko",
            "email": "prowwid@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Djaconda/code-specs.git"
        }
    ],
    "require": {
        "php": ">=7.4",
        "yidas/yii2-bower-asset": "2.0.13.1",
        "yiisoft/yii2": ">=2.0.38"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*",
        "php-kitchen/code-specs": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "PHPKitchen\\DI\\": "src",
            "PHPKitchen\\DI\\Tests\\": "tests"
        }
    }
}
